{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "NLog": {
    "throwConfigExceptions": true,
    "extensions": [
      { "assembly": "NLog.Web.AspNetCore" },
      { "assembly": "Microsoft.ApplicationInsights.NLogTarget" }
    ],
    "targets": {
      "aiTarget": {
        "type": "ApplicationInsightsTarget",
        "instrumentationKey": ""
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "aiTarget"
      }
    ]
  },
